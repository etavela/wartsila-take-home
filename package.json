{
  "name": "wartsila-take-home",
  "version": "0.1.0",
  "description": "Take home coding exercise for Wartsila",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "config": {
    "audit_level": "null"
  },
  "scripts": {
    "audit": "npm audit --production --audit-level=$npm_package_config_audit_level",
    "build": "npm run tsc",
    "build:clean": "npm run clean && npm run build",
    "check-types": "tsc --noEmit",
    "clean": "npx rimraf dist coverage",
    "lint": "eslint --ext ts --config .eslintrc.json src",
    "lint:fix": "npm run lint -- --fix",
    "qc": "npm run lint && npm run audit && npm run check-types && npm run test:coverage",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etavela/wartsila-take-home.git"
  },
  "author": "Eric Tavela",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/etavela/wartsila-take-home/issues"
  },
  "homepage": "https://github.com/etavela/wartsila-take-home#readme",
  "devDependencies": {
    "@greenlight-coop/eslint-config-greenlight": "^0.1.12",
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^28.1.6",
    "@types/node": "^16.11.45",
    "jest": "^28.1.3",
    "jest-extended": "^3.0.1",
    "jest-mock-extended": "^2.0.7",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "prettier": "@greenlight-coop/prettier-config-greenlight"
}
